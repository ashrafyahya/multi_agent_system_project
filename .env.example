# =============================================================================
# Competitor Analysis Multi-Agent System - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your actual values
# cp .env.example .env
# =============================================================================

# -----------------------------------------------------------------------------
# REQUIRED CONFIGURATION
# -----------------------------------------------------------------------------

# Groq API key for LLM service (REQUIRED)
# Get your API key from: https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here


# Tavily API key for web search (REQUIRED)
# Get your API key from: https://tavily.com/
TAVILY_API_KEY=your_tavily_api_key_here



# -----------------------------------------------------------------------------
# LANGSMITH CONFIGURATION (Optional)
# -----------------------------------------------------------------------------

# Enable LangSmith tracing for debugging and monitoring
# Get your API key from: https://smith.langchain.com/
LANGSMITH_ENABLED=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=multi_agents_system

# -----------------------------------------------------------------------------
# LLM MODEL CONFIGURATION
# -----------------------------------------------------------------------------

# Default LLM model for all agents (fallback if agent-specific model not set)
# Recommended: llama-3.1-8b-instant (fast) or llama-3.3-70b-versatile (better quality)
# LLM_MODEL=llama-3.1-8b-instant
LLM_MODEL=moonshotai/kimi-k2-instruct-0905

# Optional: Override models for specific agents
# Use faster models for planning/coordination, larger models for analysis
# LLM_MODEL_PLANNER=llama-3.1-8b-instant
# LLM_MODEL_SUPERVISOR=llama-3.1-8b-instant
# LLM_MODEL_INSIGHT=llama-3.3-70b-versatile
# LLM_MODEL_REPORT=llama-3.3-70b-versatile
# LLM_MODEL_COLLECTOR=llama-3.1-8b-instant
# LLM_MODEL_EXPORT=llama-3.3-70b-versatile

# -----------------------------------------------------------------------------
# BASIC CONFIGURATION
# -----------------------------------------------------------------------------

# Maximum number of retry attempts (range: 1-10)
MAX_RETRIES=3

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Directory for storing temporary data
DATA_DIR=./data

# -----------------------------------------------------------------------------
# AGENT LOGGING CONFIGURATION
# -----------------------------------------------------------------------------

# Directory for storing agent output log files
AGENT_LOG_DIR=./data/agent_logs

# Enable/disable agent output logging (true/false)
AGENT_LOG_ENABLED=true

# -----------------------------------------------------------------------------
# VALIDATION THRESHOLDS
# -----------------------------------------------------------------------------
# Adjust these thresholds to control quality standards at each workflow stage

# Minimum number of total insights required (SWOT items + trends + opportunities + positioning)
# Range: 1-100
MIN_INSIGHTS=8

# Minimum character length for positioning statement
# Range: 10-1000
MIN_POSITIONING_LENGTH=50

# Minimum total character length for report
# Range: 100-100000
MIN_REPORT_LENGTH=1200

# Minimum items per SWOT category (strengths, weaknesses, opportunities, threats)
# Range: 1-50
MIN_SWOT_ITEMS_PER_CATEGORY=2

# Minimum number of trends required
# Range: 1-50
MIN_TRENDS=2

# Minimum number of opportunities required (beyond SWOT)
# Range: 1-50
MIN_OPPORTUNITIES=2

# Minimum number of unique competitor sources required
# Range: 1-100
MIN_COLLECTOR_SOURCES=4

# -----------------------------------------------------------------------------
# RATE LIMITING AND RETRY CONFIGURATION
# -----------------------------------------------------------------------------

# Maximum number of retry attempts for LLM API calls
# Range: 1-10
LLM_RETRY_ATTEMPTS=3

# Minimum backoff time in seconds for exponential backoff
# Range: 0.1-60.0
LLM_RETRY_BACKOFF_MIN=1.0

# Maximum backoff time in seconds for exponential backoff
# Range: 1.0-300.0
LLM_RETRY_BACKOFF_MAX=30.0

# Enable/disable intelligent retry using LLM to analyze validation errors and improve queries
# When disabled, uses simple rule-based query enhancement
# true/false
INTELLIGENT_RETRY_ENABLED=true

# -----------------------------------------------------------------------------
# INPUT VALIDATION CONFIGURATION
# -----------------------------------------------------------------------------

# Maximum character length for user queries
# Range: 100-50000
MAX_QUERY_LENGTH=5000

# Minimum character length for user queries
# Range: 1-1000
MIN_QUERY_LENGTH=10

# -----------------------------------------------------------------------------
# LLM CACHING CONFIGURATION
# -----------------------------------------------------------------------------

# Enable/disable in-memory caching for LLM responses
# Caching reduces redundant API calls and improves performance
# true/false
LLM_CACHE_ENABLED=true

# Maximum number of cached LLM responses (LRU cache size)
# Range: 1-10000
LLM_CACHE_SIZE=128

# -----------------------------------------------------------------------------
# METRICS AND MONITORING CONFIGURATION
# -----------------------------------------------------------------------------

# Enable/disable metrics tracking for execution time, token usage, and API calls
# true/false
METRICS_ENABLED=true

# Directory path for exporting metrics data (JSON files)
METRICS_EXPORT_PATH=./data/metrics

# -----------------------------------------------------------------------------
# ASYNC OPERATIONS CONFIGURATION
# -----------------------------------------------------------------------------

# Enable/disable async execution for I/O-bound operations (LLM calls, web requests)
# When enabled, agents and tools will use async versions for parallel execution
# This significantly improves performance for multiple operations
# true/false
USE_ASYNC=false

# -----------------------------------------------------------------------------
# STATE STORAGE CONFIGURATION
# -----------------------------------------------------------------------------

# Enable/disable external storage for large state data (reports, collected data)
# When enabled, large data is stored externally and state contains references
# This reduces memory usage and improves performance
# true/false
STATE_STORAGE_ENABLED=false

# Directory for storing large state data externally
STATE_STORAGE_DIR=./data/state_storage

# Time to live for stored state data in seconds
# Data older than TTL is automatically cleaned up
# Range: 60-604800 (1 minute to 7 days)
# Default: 86400 (24 hours)
STATE_STORAGE_TTL=86400

# =============================================================================
# NOTES
# =============================================================================
# - All paths are relative to the project root directory
# - Boolean values can be: true, false, True, False, 1, 0, yes, no
# - Paths will be automatically created if they don't exist
# - See README.md for detailed documentation on each configuration option
# =============================================================================

